<local:Page  x:Class="SWAM.Controls.Pages.AdministratorPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:temp="clr-namespace:SWAM.Controls.Templates.AdministratorPage"
        xmlns:local="clr-namespace:SWAM"
        Background="{StaticResource BackgroundOfPagesBrash}"
        Padding="10"
        mc:Ignorable="d">
    <StackPanel>
        <!--#region TemporaryNavigationBar -->
        <StackPanel Orientation="Horizontal" 
                    Name="TemporaryNavigationBar" 
                    VerticalAlignment="Bottom"
                    HorizontalAlignment="Left">
            <Button Width="130" 
                    Content="Użytkownicy" 
                    Name="SwitchToUsersControlPanel" 
                    Click="UsersControlPanelBar_Click"/>
            <Button Width="130" Content="Magazyny" 
                    Name="SwitchToWarehousesControlPanel" 
                    Click="WarhousesControlPanelBarPage_Click"/>
        </StackPanel>
        <!--#endregion-->
        <StackPanel Orientation="Vertical" Name="MainContent">
            <temp:UsersControlPanelTemplate />
        </StackPanel>
    </StackPanel>

</local:Page>
